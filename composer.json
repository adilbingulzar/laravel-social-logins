{
    "name": "social/logins",
    "description": "This package provides social login functionality for Laravel applications.",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Social\\Logins\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Adil",
            "email": "adilbingulzar@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Social\\Logins\\LoginServiceProvider"
            ]
        }
    },
    "require": {
        "illuminate/support": "^9.21|^10.0",
        "laravel/socialite": "^5.0"
    },
    "scripts": {
        "post-install-cmd": [
            "@php artisan migrate --path=vendor/social/logins/database/migrations"
        ],
        "post-update-cmd": [
            "@php artisan migrate --path=vendor/social/logins/database/migrations"
        ]
    }
}
